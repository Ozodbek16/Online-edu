<form action="/api/category/update/{{category._id}}">
    <label for="name">Category name</label>
    <input value="{{category.name}}" type="text" id="name" name="name">

    <label for="img">Category img</label>
    <input value="{{category.img}}" type="text" id="img" name="img">

    <button type="submit">Create</button>
</form>